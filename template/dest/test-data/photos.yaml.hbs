photos:
{{~#unless photos}}
  []
{{else}}
{{#each photos as |photo| ~}}
- id: {{photo.id}}
  album_id: {{photo.albumId}}
  title: |
    {{shift photo.title 4}}
  url: |
    {{photo.url}}
  thumbnail_url: |
    {{photo.thumbnailUrl}}
  date_taken: |
    {{fake 'date-in-last-decade' "yyyy-MM-dd'T'hh:ss:mm"}}
{{/each}}
{{/unless}}
